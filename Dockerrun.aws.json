{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "cenitodoo",
      "image": "cenitsaas/cenitodoo",
      "environment": [
        {
          "name": "MAIN_DOMAIN",
          "value": "cenitodoo.com"
        },
        {
          "name": "SERVER_SUBDOMAIN",
          "value": "server.cenitodoo.com"
        },
        {
          "name": "CUSTOM_MODULES",
          "value": "https://github.com/dhbahr/odoo-cenit.git"
        }
      ],
      "essential": true,
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8069,
          "protocol": "tcp"
        }
      ],
      "links": [],
      "mountPoints": []
    }
  ]
}
