{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "db",
      "image": "postgres",
      "environment": [
        {
          "name": "POSTGRES_USER",
          "value": "odoo"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "odoo"
        }
      ],
      "essential": true,
      "memory": 300,
      "portMappings": [],
      "links": [],
      "mountPoints": []
    },
    {
      "name": "cenitodoo",
      "image": "cenitsaas/cenitodoo",
      "environment": [
        {
          "name": "MAIN_DOMAIN",
          "value": "cenitodoo.com"
        },
        {
          "name": "SERVER_SUBDOMAIN",
          "value": "server.cenitodoo.com"
        },
        {
          "name": "CUSTOM_REPOS",
          "value": "https://github.com/dhbahr/odoo-cenit.git https://github.com/dhbahr/odoo-integrations.git"
        }
      ],
      "essential": true,
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8069,
          "protocol": "tcp"
        }
      ],
      "links": [
          "db"
      ],
      "mountPoints": []
    }
  ]
}
